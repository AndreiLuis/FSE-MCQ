        {
          "Host": "Policy.Api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/gateway/v1.0/policy/{everything}",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET" ],
      "SwaggerKey": "policy"
    },
    {
      "DownstreamPathTemplate": "/api/v1.0/user/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
